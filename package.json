{
    "name": "vscode-cljfmt",
    "displayName": "cljfmt",
    "description": "An extension for formatting Clojure & ClojureScript code",
    "version": "0.0.3",
    "publisher": "pedrorgirardi",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "icon": "images/icon.png",
    "activationEvents": [
        "onLanguage:clojure"
    ],
    "main": "./extension",
    "contributes": {},
    "scripts": {
        "watch": "npx shadow-cljs watch :extension",
        "compile": "npx shadow-cljs compile :extension",
        "clean": "rm -rf .shadow-cljs/",
        "vscode:prepublish": "npm run clean && npx shadow-cljs release :extension",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^4.11.0",
        "shadow-cljs": "^2.3.23",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}
